<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="bridgelabz.app.com.listviewswipetest">

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".ui.SplashActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name=".ui.LoginActivity" />
        <activity android:name=".ui.RegisterationActivity" />
        <activity
            android:name=".ui.TodoHomeActivity"
            android:theme="@style/Theme.Design.NoActionBar" />
        <activity android:name=".ui.NoteActivity" />
        <activity android:name=".ui.TodoNoteAddActivity" />
    </application>

</manifest>